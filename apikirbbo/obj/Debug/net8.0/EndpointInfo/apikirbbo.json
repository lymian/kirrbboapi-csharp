  "components": {
    "schemas": {
      "CompraDetalleDTO": {
        "required": [
          "cantidad",
          "productoId"
        ],
        "type": "object",
        "properties": {
          "productoId": {
            "type": "integer",
            "format": "int32"
          },
          "cantidad": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "CompraRequest": {
